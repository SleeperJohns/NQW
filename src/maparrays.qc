float() StartMaps =
{
	if (world.model == "maps/start.bsp")    return 1;
	if (world.model == "maps/hipstart.bsp") return 2;
	if (world.model == "maps/rstart.bsp")   return 3;
	if (world.model == "maps/e5start.bsp")  return 4; 
	if (world.model == "maps/mgstart.bsp")  return 5; 
	if (world.model == "maps/hub.bsp") 	return 6; 
	return FALSE; // Blank Map.
};

float() HordeMaps =
{
	if (world.model == "maps/horde1.bsp") return 1;
	if (world.model == "maps/horde2.bsp") return 2;
	if (world.model == "maps/horde3.bsp") return 3;
	if (world.model == "maps/horde4.bsp") return 4; 
	if (world.model == "maps/horde5.bsp") return 5; 
	if (world.model == "maps/horde6.bsp") return 6; 
	if (world.model == "maps/horde7.bsp") return 7;
	return FALSE; // Blank Map.
};
